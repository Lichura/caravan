<div class="page-header">
  <%= link_to paises_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_pais_path(@pais), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show pais</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @pais.nombre %></dd>

  <dt>Abreviacion:</dt>
  <dd><%= @pais.abreviacion %></dd>

</dl>
