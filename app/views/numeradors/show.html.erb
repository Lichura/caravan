
<div class="container">
  <div class="panel-group">
    <div class="panel panel-default">
      <div class="panel-heading"><h2>numerador</h2></div>
        <div class="panel-body">
         <dl class="dl-horizontal">
          <dt>Comprobante:</dt>
          <dd><%= @numerador.comprobante %></dd>

          <dt>Puntodeventa:</dt>
          <dd><%= @numerador.puntoDeVenta %></dd>

          <dt>Numero:</dt>
          <dd><%= @numerador.numero %></dd>

        </dl>
        </div>
      </div>
    </div>
      <%= link_to edit_numerador_path, class: 'btn btn-default pull-right' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        numeradors
      <% end %>
      <%= link_to numerador_path(@numerador), method: :delete, data: { confirm: 'Esta seguro que desea eliminar esta provincia?' }, class: 'btn btn-danger pull-right', remote: true do %>
        <span class="glyphicon glyphicon-trash"></span>
        Eliminar
      <% end %>
      <%= link_to edit_numerador_path(@numerador),  class: 'btn btn-primary' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
  </div>
</div>