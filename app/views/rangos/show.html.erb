
<div class="container">
  <div class="panel-group">
    <div class="panel panel-default">
      <div class="panel-heading"><h2>rango</h2></div>
        <div class="panel-body">
         <dl class="dl-horizontal">
          <dt>Letras:</dt>
          <dd><%= @rango.letras %></dd>

          <dt>Numero:</dt>
          <dd><%= @rango.numero %></dd>

          <dt>Digito:</dt>
          <dd><%= @rango.digito %></dd>

          <dt>Pedido:</dt>
          <dd><%= @rango.pedido_id %></dd>

          <dt>User:</dt>
          <dd><%= @rango.user_id %></dd>

        </dl>
        </div>
      </div>
    </div>
      <%= link_to edit_rango_path, class: 'btn btn-default pull-right' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        rangos
      <% end %>
      <%= link_to rango_path(@rango), method: :delete, data: { confirm: 'Esta seguro que desea eliminar esta provincia?' }, class: 'btn btn-danger pull-right', remote: true do %>
        <span class="glyphicon glyphicon-trash"></span>
        Eliminar
      <% end %>
      <%= link_to edit_rango_path(@rango),  class: 'btn btn-primary' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        Editar
      <% end %>
  </div>
</div>