<div class="page-header">
  <%= link_to medidas_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_medida_path(@medida), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show medida</h1>
</div>

<dl class="dl-horizontal">
  <dt>Codigoafip:</dt>
  <dd><%= @medida.codigoAfip %></dd>

  <dt>Nombre:</dt>
  <dd><%= @medida.nombre %></dd>

  <dt>Abreviatura:</dt>
  <dd><%= @medida.abreviatura %></dd>

  <dt>Descripcion:</dt>
  <dd><%= @medida.descripcion %></dd>

</dl>
