                <% @remitos.each do |remito| %>
                  <tr>
                    <td><%= check_box_tag "remito_ids[]", remito.id %></td>
                    <td><%= remito.pedido.comprobanteNumero %></td>
                    <td><%= remito.numero %></td>
                    <td><%= remito.pedido.cuit %></td>
                    <td><%= User.find(remito.pedido.user_id).razonSocial %></td>
                    <td><%= remito.fecha %></td>
                  </tr>
                <% end %>
