<div class="page-header">
  <%= link_to detalles_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_detalle_path(@detalle), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show detalle</h1>
</div>

<dl class="dl-horizontal">
  <dt>Pedido:</dt>
  <dd><%= @detalle.pedido.id %></dd>

  <dt>Producto:</dt>
  <dd><%= @detalle.producto_id %></dd>

  <dt>Cantidad:</dt>
  <dd><%= @detalle.cantidad %></dd>

  <dt>Precio:</dt>
  <dd><%= @detalle.precio %></dd>


    <dt>Rango Desde:</dt>
  <dd><%= @detalle.rango_desde %></dd>

    <dt>Rango Hasta:</dt>
  <dd><%= @detalle.rango_hasta %></dd>

</dl>
